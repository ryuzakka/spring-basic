<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybats-3-mapper.dtd">

<mapper namespace="kr.co.buck1.member.MemberDao">

	<select id="signup" parameterType="kr.co.buck1.member.MemberDto" resultType="int">
		select count(*) cnt from member where id=#{userid} and pwd=#{pwd}
	</select>

</mapper>